<!--src=pages/myInfo/myInfo.wxml-->
<view class="weui-cells weui-cells_after-title">
  <view class="weui-cell weui-cell_example ">
    <view class="weui-cell__hd">
      <!--src={{icon}}-->
      <image src='/images/touxiang_off.png' style="margin-right: 16px;vertical-align: middle;width:40px; height: 40px;"></image>
    </view>
    <view class="weui-cell__bd1" >微信号</view>
    <view class="weui-cell__ft">昵称</view>
  </view>
</view>
<view class="divLine"></view>

<view class="divLine"></view>
<view class="weui-cells weui-cells_after-title">
  <view class="weui-cell">
    <view class="weui-cell__bd">姓名</view>
    <view class="weui-cell__ft">{{name}}</view>
  </view>
</view>

<view class="divLine"></view>
<view class="weui-cells weui-cells_after-title">
  <view class="weui-cell">
    <view class="weui-cell__bd">学号</view>
    <view class="weui-cell__ft">{{num}}</view>
  </view>
</view>
<view class="divLine"></view>

<view class="divLine"></view>
<view class="weui-cells weui-cells_after-title">
  <view class="weui-cell">
    <view class="weui-cell__bd">学院</view>
    <view class="weui-cell__ft">{{academy}}</view>
  </view>
</view>
<view class="divLine"></view>

<view class="divLine"></view>
<view class="weui-cells weui-cells_after-title">
  <view class="weui-cell">
    <view class="weui-cell__bd">专业</view>
    <view class="weui-cell__ft">{{major}}</view>
  </view>
</view>
<view class="divLine"></view>

<view class="weui-cells weui-cells_after-title">
  <view class="weui-cell">
    <view class="weui-cell__bd">手机号</view>
    <view class="weui-cell__ft">{{tel}}</view>
  </view>
</view>
<view class="divLine"></view>
<view class="divLine"></view>

<view class="weui-cells weui-cells_after-title">
  <navigator url="/pages/favorite/favorite" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
    <view class="weui-cell__bd">收藏</view>
    <view class="weui-cell__ft weui-cell__ft_in-access"></view>
  </navigator>
</view>
<view class="divLine"></view>

<view class="weui-cells weui-cells_after-title">
  <navigator url="/pages/modify/modify" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
    <view class="weui-cell__bd">编辑资料</view>
    <view class="weui-cell__ft weui-cell__ft_in-access"></view>
  </navigator>
</view>
<view class="divLine"></view>